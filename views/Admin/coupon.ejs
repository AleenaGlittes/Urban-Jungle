<section class="content-main">
    
    <section class="content-main">
        <div class="row">
            <div class="col-9">
                <div class="content-header">
                    <h2 class="content-title">Add New Coupon</h2>
                </div>
            </div>
            <!-- <button class="btn btn-md rounded font-sm hover-up"  type="submit" >Add</button> -->
            
            <div class="col-lg-6">
                <div class="card mb-4">
                    <div class="card-header">
                        <h4>Add Coupon</h4>
                    </div>
                    <div class="card-body">
                        <form action="/admin/addCoupon"  method="POST"  >
                            <div class="mb-4">
                                <label for="product_name" class="form-label">Coupon code</label>
                                <input type="text" name="code" placeholder="Type here"  class="form-control" id="product_name">
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="mb-4">
                                        <label class="form-label">Discount Price</label>
                                        <div class="row gx-2">
                                            <input placeholder="₹" name="discountprice" type="text" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="mb-4">
                                        <label class="form-label">Min Purchase</label>
                                        <input placeholder="₹" type="text" name="min_purchase" class="form-control">
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="mb-4">
                                        <label class="form-label">Expiry</label>
                                        <input placeholder="" type="date" name="expiry" class="form-control">
                                    </div>
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Description</label>
                                    <textarea placeholder="Type here" name="coupondescription" class="form-control" rows="4"></textarea>
                                </div>
                            </div>
                            <button class="btn btn-md rounded font-sm hover-up" type="submit">Add</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>

