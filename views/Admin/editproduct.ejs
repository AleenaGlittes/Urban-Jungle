<section class="content-main">

        <section class="content-main">
            <form action="/admin/editproductdetails" method="post">
                
            <div class="row">
                <div class="col-9">
                    <div class="content-header">
                        <h2 class="content-title">Edit Product</h2>
                        <div>
                            <!-- <button class="btn btn-light rounded font-sm mr-5 text-body hover-up">Save to draft</button> -->
                            <button  type="submit"  class="btn btn-md rounded font-sm hover-up"><a href="">Update</a></button>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="mb-4">
                                <input type="hidden" value="<%=productdata._id%>" name="id">
                                <label for="product_title" class="form-label"></label>
                                <input type="text" class="form-control"  name="productname" id="product_title" value="<%=productdata.productname%>">
                                <div class="mb-4">
                                    <label for="product_category" class="form-label">Category</label>
                                    <input type="text" class="form-control" name="category"  id="product_category"value="<%=productdata.category%>">
                                </div>
                            <div class="mb-4">
                                <label for="product_brand" class="form-label"></label>
                                <input type="text" class="form-control" name="brand"  id="product_brand" value="<%=productdata.brand%>">
                            </div>
                            <div class="mb-4">
                                <label for="product_brand" class="form-label"></label>
                                <input type="text" class="form-control" name="quantity"  id="product_brand" value="<%=productdata.quantity%>">
                            </div>
                            <div class="mb-4">
                                <label for="product_price" class="form-label"></label>
                                <input type="text" class="form-control" name="price"   id="product_price" value="<%=productdata.price%>">
                            </div>
                           
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <div>
                                <label class="form-label"></label>
                                <textarea class="form-control"  name="description" rows="4"><%=productdata.description%></textarea>
                            </div>
                        </div>
                    </div> <!-- card end// -->
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="input-upload">
                                <img src="/upload/<%=productdata.productimage[0] %>" alt="">
                                
                                <input class="form-control" type="file" name="productImage" multiple>
                            </div>
                    </div> <!-- card end// -->
                </div>
           
                
                </div> 
            </div>
            </form>
            
        </section>
      
    </main>
